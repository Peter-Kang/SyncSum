<h1>Listing correct_answers</h1>

<table>
  <tr>
    <th>Questionid</th>
    <th>Integer</th>
    <th>Question answerid</th>
    <th>Employer userid</th>
    <th>Integer</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @correct_answers.each do |correct_answer| %>
  <tr>
    <td><%= correct_answer.questionID %></td>
    <td><%= correct_answer.integer %></td>
    <td><%= correct_answer.question_answerID %></td>
    <td><%= correct_answer.employer_userID %></td>
    <td><%= correct_answer.integer %></td>
    <td><%= link_to 'Show', correct_answer %></td>
    <td><%= link_to 'Edit', edit_correct_answer_path(correct_answer) %></td>
    <td><%= link_to 'Destroy', correct_answer, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

