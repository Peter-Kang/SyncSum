<h1>Concentrations</h1>
<p>
  <b>Name:</b>
  <%= @concentration.name %>
  
  <% if @concentration.business_sectors %>
    <b>Business Sectors: </b>
    <% @concentration.business_sectors.each do |bs| %>
      <%= bs.name%>
    <% end %>
  <% end %>
</p>
<%= render :partial => 'business_sectors/form1', :locals => { :business_sector => @concentration.business_sectors.new} %>



<%= link_to 'Edit', edit_concentration_path(@concentration) %> |
<%= link_to 'Back', concentrations_path %> |
<%= link_to 'New', new_concentration_path %>