<%= form_for(education) do |f| %>
	<%= f.label :degree_id %>
	<%= f.collection_select :degree_id, Degree.all, :id, :name, :prompt => "Degree:" %>
	<%= f.label :concentration_id %>
	<%= f.collection_select :concentration_id, Concentration.all, :id, :name, :prompt => "Concentration:" %>
	<%= f.submit :value => "Add Education", :class => "button2" %>
<% end %>
